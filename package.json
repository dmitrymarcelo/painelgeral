{
  "name": "frota-pro",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "format": "turbo run format",
    "db:generate": "pnpm --filter @frota/api prisma:generate",
    "db:migrate": "pnpm --filter @frota/api prisma:migrate",
    "db:seed": "pnpm --filter @frota/api seed"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  }
}